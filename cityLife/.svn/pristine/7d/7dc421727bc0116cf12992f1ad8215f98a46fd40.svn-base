	body{
    	padding: 0;
    	font:12px/1.5 tahoma,arial,'Hiragino Sans GB','\5b8b\4f53',sans-serif;
    	background: #f4f4f4;
    	min-width:320px;
    	max-width: 700px;
    	margin: 0 auto;
    }
    
    header{
    	min-height:45px;
    	background-image: url("../img/bg_title.png");
    	/* background-color:#3CB371; */
    	/* -webkit-border-radius: 5px; */
    }
    
    .return{
    	float:left;
    }
    
    .share{
    	float: right;
    }
    
    .comment{
    	float: right;
    }
    
    .title{
    	border-bottom: 1px solid #ccc ;
    	
    }
    
    .head{
    	height: 62px;
    	margin: 10px;
    	border-bottom: 1px ;
    }
    
    .left{
    	float: left;
    	margin-right: 5px;
    }
    
    .right {
    	width:100%;
    	height:62px;
    }
    
    .right h3{
    	margin: 0;
    	height: 22px;
    }
    
    .star{
    	height: 20px;
    }
    
    .bottom {
		height: 20px;
	}
    
    .preferen{
    	color:red;
    	font-size: 15px;
    }
    
    .phoneBg{
    	float: right;
		width: 100px;
		height: 22px;
		background-image: url("../img/phone.png");
		background-repeat: no-repeat;
		background-size: 100px 22px;
    }
    
    .phone{
    	color:white;
    	margin:2px 0 0 23px;
    	display: block;
    }
    
    