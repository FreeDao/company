	body{
    	padding: 0;
    	font:12px/1.5 tahoma,arial,'Hiragino Sans GB','\5b8b\4f53',sans-serif;
    	background: #f4f4f4;
    	min-width:320px;
    	max-width: 700px;
    	margin: 0 auto;
    }
    
    header{
    	height:45px;
    	background-image: url("../img/bg_title.png");
    	/* background-color:#3CB371; */
    	/* -webkit-border-radius: 5px; */
    }
    
    a{
    	text-decoration: none;
    	cursor: pointer;
    }
    
    .topBar{
    	max-width:700px;
    	width:100%;
    	position: fixed;
    }
    
    .return{
    	float:left;
    }
    
    .share{
    	margin-top:4px;
    	float: right;
    }
    
    .comment{
    	margin-top:5px;
    	float: right;
    }

    .title{
    	background-color:#f4f4f4;
    	box-shadow: 2px 2px 2px #ccc;
    	height: 72px;
		padding: 10px 10px 0 10px;
		filter:alpha(opacity=95); /*IE滤镜，透明度50%*/
		-moz-opacity:0.95; /*Firefox私有，透明度50%*/
		opacity:0.95;/*其他，透明度50%*/
}
    
    .head{
    	height: 62px;
    }
    
    .left{
    	float: left;
    	margin-right: 5px;
    }
    
    img{
    	-webkit-border-radius: 5px;
    }
    
    .right {
    	width:100%;
    	height:62px;
    }
    
    .right h3{
    	margin: 0;
    	height: 22px;
    }
    
    .star{
    	height: 20px;
    }
    .star img{
    	margin-right: -3px;
    }
    .starBottom img{
    	margin-left: -3px;
    }
    
    .bottom {
		height: 20px;
	}
    
    .preferen{
    	color:red;
    	font-size: 15px;
    }
    
    .phoneBg{
    	text-decoration:none;
    	float: right;
		width: 100px;
		height: 22px;
		background-image: url("../img/phone.png");
		background-repeat: no-repeat;
		background-size: 100px 22px;
    }
    
    .phone{
    	color:white;
    	margin:2px 0 0 23px;
    	display: block;
    }
    
    .content{
    	padding-top: 127px;
    	/* height:200px;
    	overflow: hidden; */
    }
    
    
    .address{
    	height: 50px;
    	border-bottom: 1px solid #ccc;
    }
    .address img{
    	float: left;
    }
    .address div{
    	padding: 13px;
    }
    .address span{
    	display: inline-block;
    	white-space: nowrap;
    	text-overflow: ellipsis;
    	width:85%;
    	overflow:hidden;
    	font-size:15px;
    	margin-left: 5px;
    }
    .address img:nth-child(1){
    	float: right;
    	margin-top: 8px;
    }
    
    .searchway{
    	height: 50px;
    	border-bottom: 1px solid #ccc;
    }
    .searchway img{
    	float: left;
    }
    .searchway div{
    	padding: 13px;
    }
    .searchway span{
    	display: inline-block;
    	white-space: nowrap;
    	text-overflow: ellipsis;
    	width:85%;
    	overflow:hidden;
    	font-size:15px;
    	margin-left: 5px;
    }
     .searchway img:nth-child(1){
    	float: right;
    	margin-top: 8px;
    }
    
    .comment_content div{
    	text-align: right;
    }
    .comment_content span{
    	color: #aaa;
    }
    .comment_content p , .comment_content span{
    	line-height: 3px;
    }
    
    .service,.brief{
    	margin: 13px;
    }
    
     .service p,.brief p{
    	color: #aaa;
    }
    
    .comment_title{
    	margin-left: 13px;
    	font-size: 15px;
    }
    
    .comment_content{
    	margin: 0px 10px;
    }
    
    .imgs{
    	width:62px;
    	height:50px;
    	border: 1px solid #ccc;
    	padding: 2px;
    	margin: 2px;
    	box-shadow:2px 2px 2px #ccc;
    }
    
    .imgChange{
    	/* display:-webkit-box;
    	-webkit-box-align: center;
		-webkit-box-pack: center; */
		/* display: none;  */
    	z-index:2;
		position: fixed;
		overflow:hidden;
		/* top:0;
		bottom:0;
		left:0;
		right:0; */
		max-width: 700px;   
		background-color: #000;
    }
    
    .addWrap{
    	height: 100%;
    }
    
    .swipe{
    	height: 100%;
   	 	display:-webkit-box;
   		-webkit-box-align: center;
		-webkit-box-pack: center;
		
		display:-moz-box;
   		-moz-box-align: center;
		-moz-box-pack: center;
    }
    
    .swipe-wrap{
    	width: 100%;
    	max-height: 700px;
    }
    
    .bigImgs{
    	width:100%;
    	height:300px;
    	max-width: 700px;
    }
