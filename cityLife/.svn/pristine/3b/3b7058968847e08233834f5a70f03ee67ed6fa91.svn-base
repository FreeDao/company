 	body{
    	padding: 0;
    	font:12px/1.5 tahoma,arial,'Hiragino Sans GB','\5b8b\4f53',sans-serif;
    	background: #f4f4f4;
    	min-width:320px;
    	max-width: 700px;
    	margin: 0 auto;
    }
    
    .header{
    	min-height:45px;
    	background-image: url("../img/bg_title.png");
    	/* background-color:#3CB371; */
    	/* -webkit-border-radius: 5px; */
    }
    
    .header .refresh{
    	float: right;
    }
    
    .header .city{
    	margin: 13px 0 0 5px;
    	float:left;
    	color:white;
    }
    
    select{
    color:white;
    border: none;
    background-color: transparent;
    -webkit-appearance: none;
    
    font: -webkit-small-control;
	letter-spacing: normal;
	word-spacing: normal;
	text-transform: none;
	text-indent: 0px;
	text-shadow: none;
	display: inline-block;
	text-align: start;
    }
    
    .header h3{
    	font-size:19px;
    	color: white;
    	width: 40px;
    	margin: 0 auto;
    	padding-top: 13px;
    }
    
    .search{
    	background: #E4E2E2;
    	padding: 5px 0px;
    }
    
    .search .searchBar{
    	background-color: #ffffff;
    	border : 1px solid #ccc;
    	-webkit-border-radius: 25px;
    	width: 94%;
    	margin:0 3%;
    }
    
    .search input{
    	border: 0;
    	-webkit-border-radius: 25px;
    	height:25px;
    	width: 79%;
    	
    }
    
    .search .searchBar a{ 
    	float : right;
    	margin-top:4px;
    	height:18px;
    	border-left: 1px solid #ccc;
    }
    
    .search .searchBar img{ 
    	margin: 3px 5px 0 6px;
    }
    
    .imgCell{
    	width:75px;
    	height:75px ;
    	display:inline-block;
    	margin-top:6px;
    }
    
    .imgCell label{
    	position: absolute;
    	margin:22px 22px;
    	color: #007A5D;
    	width:40px;
    }
    
